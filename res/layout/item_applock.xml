<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="8dp" >

    <ImageView
        android:id="@+id/item_applock_iv_icon"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_centerVertical="true"
        android:src="@drawable/ic_default" />

    <ImageView
        android:id="@+id/item_applock_iv_lock"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:src="@drawable/btn_lock_selector" />

    <TextView
        android:id="@+id/item_applock_tv_name"
        style="@style/NormalTextStyle"
        android:layout_centerVertical="true"
        android:layout_marginLeft="8dp"
        android:layout_toLeftOf="@id/item_applock_iv_lock"
        android:layout_toRightOf="@id/item_applock_iv_icon"
        android:singleLine="true"
        android:text="名字" />

</RelativeLayout>